{
  "name": "basscss-logo",
  "version": "1.0.0",
  "description": "React component and SVG logo for Basscss",
  "main": "Logo.js",
  "scripts": {
    "babel": "babel src --out-dir .",
    "watch": "babel src -w --out-dir .",
    "svg": "node scripts/svg",
    "png": "node scripts/png",
    "images": "npm run svg && npm run png",
    "dev": "webpack-dev-server --progress --colors",
    "demo": "webpack -p --progress --colors",
    "start": "npm run watch & npm run dev"
  },
  "author": "Brent Jackson",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.14",
    "babel-loader": "^5.2.2",
    "colors.css": "^2.3.0",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.7",
    "static-site-generator-webpack-plugin": "^1.0.0",
    "svg-to-png": "^2.0.0",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  }
}
